{
  "name": "jquery.l10n",
  "version": "1.0.2",
  "description": "A simple localisation jQuery plugin, that can be used in static sites.",
  "main": "jquery.l10n.js",
  "scripts": {
    "babel": "babel --presets es2015 jquery.l10n.js -o dist/jquery.l10n.js",
    "start": "http-server",
    "webpack": "webpack"
